set nocompatible
set backspace=2
filetype plugin indent on
syntax on
set encoding=utf8
let mapleader=","
set ruler
set number
set background=dark
set showmode
set showcmd
set title
set laststatus=2
set ignorecase
set smartcase
set hlsearch
set incsearch
set nobackup
set nowb
set noswapfile
set tabstop=4
set shiftwidth=4
set cindent
set expandtab
